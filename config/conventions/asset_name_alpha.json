{
  "style": "upper_camel_case",
  "nullable": false,
  "structure": "(name)(variant)",
  "groups": {
    "name": {
      "characters": "a-zA-Z",
      "validators": [
        {
          "type": "dictionary",
          "words": ["nouns"]
        }
      ]
    },
    "variant": {
      "characters": "a-zA-Z",
      "validators": [
        {
          "filename": "regex_single_alpha_character"
        }
      ]
    }
  }
}